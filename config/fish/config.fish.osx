set -x EDITOR vim

if status --is-login
    set -x PATH ./node_modules/.bin ~/.bin ~/.local/bin $PATH
end

set -x WORKON_BASE_DIR "$HOME/Documents"

ulimit -n 4096

set -x GOPATH ~/.go

begin
    set vf_eval (python -m virtualfish auto_activation 2>/dev/null); and eval $vf_eval
end 2>&1 >/dev/null


# Config.d
begin
    source ~/.config/fish/config.d/**.fish
end 2>&1 >/dev/null
